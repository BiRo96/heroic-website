{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "root": true,
    "rules": {
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-explicit-any": "error",
      "@typescript-eslint/no-unused-vars": "error",
      "complexity": "off",
      "quotes": [
        "error",
        "single",
        { "avoidEscape": true, "allowTemplateLiterals": true }
      ]
    }
  }
  
